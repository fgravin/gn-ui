{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "apps",
  "projects": {
    "app-search": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/search",
      "sourceRoot": "apps/search/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/search",
            "index": "apps/search/src/index.html",
            "main": "apps/search/src/main.ts",
            "polyfills": "apps/search/src/polyfills.ts",
            "tsConfig": "apps/search/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/search/src/favicon.ico",
              "apps/search/src/assets"
            ],
            "styles": [
              "libs/ui/src/styles.css",
              "apps/search/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/search/src/environments/environment.ts",
                  "with": "apps/search/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "en": {
              "aot": true,
              "outputPath": "dist/app-search-en",
              "i18nFile": "apps/search/src/i18n/messages.en.xlf",
              "i18nFormat": "xlf2",
              "i18nLocale": "en"
            },
            "fr": {
              "aot": true,
              "outputPath": "dist/app-search-fr",
              "i18nFile": "apps/search/src/i18n/messages.fr.xlf",
              "i18nFormat": "xlf2",
              "i18nLocale": "fr"
            },
            "de": {
              "aot": true,
              "outputPath": "dist/app-search-de",
              "i18nFile": "src/i18n/messages.de.xlf",
              "i18nFormat": "xlf2",
              "i18nLocale": "de"
            },
            "es": {
              "aot": true,
              "outputPath": "dist/app-search-es",
              "i18nFile": "src/i18n/messages.es.xlf",
              "i18nFormat": "xlf2",
              "i18nLocale": "es"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-search:build",
            "proxyConfig": "proxy-config.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-search:build:production"
            },
            "en": {
              "browserTarget": "app-search:build:en"
            },
            "fr": {
              "browserTarget": "app-search:build:fr"
            },
            "de": {
              "browserTarget": "app-search:build:de"
            },
            "es": {
              "browserTarget": "app-search:build:es"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-search:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "detectOpenHandles": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/search/tsconfig.app.json",
              "apps/search/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "libs/gn-api/**"
            ]
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {
            "xliffmergeOptions": {
              "i18nFormat": "xlf2",
              "srcDir": "apps/search/src/",
              "genDir": "apps/search/src/i18n",
              "defaultLanguage": "en",
              "languages": [
                "en",
                "fr",
                "de",
                "es"
              ],
              "beautifyOutput": true,
              "allowIdChange": false,
              "autotranslate": false,
              "verbose": false,
              "quiet": false
            }
          }
        }
      }
    },
    "search-wc": {
      "projectType": "application",
      "schematics": {},
      "root": "webcomponents/search-wc",
      "sourceRoot": "webcomponents/search-wc/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "webcomponents/search-wc/dist",
            "index": "webcomponents/search-wc/src/index.html",
            "main": "webcomponents/search-wc/src/main.ts",
            "polyfills": "webcomponents/search-wc/src/polyfills.ts",
            "tsConfig": "webcomponents/search-wc/tsconfig.app.json",
            "aot": true,
            "assets": [
              "webcomponents/search-wc/src/favicon.ico",
              "webcomponents/search-wc/src/assets"
            ],
            "styles": [
              "webcomponents/search-wc/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "webcomponents/search-wc/src/environments/environment.ts",
                  "with": "webcomponents/search-wc/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "500kb",
                  "maximumError": "1024kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "search-wc:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "search-wc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "search-wc:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "webcomponents/search-wc/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "libs/gn-api/**"
            ]
          }
        }
      }
    },
    "lib-search": {
      "projectType": "library",
      "root": "libs/search",
      "sourceRoot": "libs/search/src",
      "prefix": "search",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/search/tsconfig.lib.json",
            "project": "libs/search/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/search/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "detectOpenHandles": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/search/tsconfig.lib.json",
              "libs/search/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "libs/gn-api/**"
            ]
          }
        }
      }
    },
    "lib-ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/ui/tsconfig.lib.json",
            "project": "libs/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "detectOpenHandles": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "libs/gn-api/**"
            ]
          }
        }
      }
    },
    "lib-gn-api": {
      "projectType": "library",
      "root": "libs/gn-api",
      "sourceRoot": "libs/gn-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/gn-api/tsconfig.lib.json",
            "project": "libs/gn-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/gn-api/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "ssr-formatter": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "ssr/formatter",
      "sourceRoot": "ssr/formatter/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/formatter/browser",
            "index": "ssr/formatter/src/index.html",
            "main": "ssr/formatter/src/main.ts",
            "polyfills": "ssr/formatter/src/polyfills.ts",
            "tsConfig": "ssr/formatter/tsconfig.app.json",
            "aot": true,
            "assets": [
              "ssr/formatter/src/favicon.ico",
              "ssr/formatter/src/assets"
            ],
            "styles": [
              "ssr/formatter/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "ssr/formatter/src/environments/environment.ts",
                  "with": "ssr/formatter/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ssr-formatter:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ssr-formatter:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ssr-formatter:build"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/ssr-formatter/server",
            "main": "ssr/formatter/server.ts",
            "tsConfig": "ssr/formatter/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "ssr/formatter/src/environments/environment.ts",
                  "with": "ssr/formatter/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "ssr-formatter:build",
            "serverTarget": "ssr-formatter:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "ssr-formatter:build:production",
              "serverTarget": "ssr-formatter:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "ssr-formatter:build:production",
            "serverTarget": "ssr-formatter:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "lib-common": {
      "projectType": "library",
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/common/tsconfig.lib.json",
            "project": "libs/common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/common/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "detectOpenHandles": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/common/tsconfig.lib.json",
              "libs/common/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "app-search"
}